
rules_version = '2';

service firebase.storage {
  match /b/{bucket}/o {
    // WARNING: These rules are insecure and are for development purposes only.
    // They allow anyone to upload, read, and delete files in your storage.
    // You MUST secure your storage before deploying to production.
    match /{allPaths=**} {
      allow read, write: if true;
    }
  }
}
